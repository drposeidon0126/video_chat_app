{
  "npmScope": "peek",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packs"
  },
  "projects": {
    "peek": {
      "tags": [],
      "implicitDependencies": ["shared-assets"]
    },
    "peek-e2e": {
      "tags": [],
      "implicitDependencies": ["peek"]
    },
    "core-model": {
      "tags": []
    },
    "seek": {
      "tags": []
    },
    "shared-assets": {
      "tags": []
    },
    "shared-styles": {
      "tags": []
    },
    "core-peek": {
      "tags": []
    },
    "core-usecase": {
      "tags": []
    },
    "micro-upgrade": {
      "tags": []
    }
  }
}
